(define (serie op fct a inc b)
  (if (> a b)
      (if (equal? op +) 0 1)
      (op (fct a) (serie op fct (inc a) inc b))))

(define (carre x)(* x x))
(define (incr x)(+ x 1))
(define (identite x) x)
(define (racine x)(sqrt x))
(serie * carre 2 incr 5)
(serie + incr 2 carre 5)
(serie * identite 2 incr 5)
(serie * racine 4 carre 6)